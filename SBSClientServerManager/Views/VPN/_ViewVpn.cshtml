
@model SBSClientServerManager.Models.ViewModels.VpnFormViewModel
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")"
        type="text/javascript">
</script>

<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"
        type="text/javascript">
</script>

@using (Html.BeginForm())
{

    <div class="form-group">
        <div class="form-group">
            @Html.LabelFor(s => s.Name)
            @Html.DisplayFor(s => s.Name, new { @class = "form-control", @Value = Model.Name })
            @Html.ValidationMessageFor(s => s.Name)
        </div>
        <div class="form-group">
            @Html.LabelFor(s => s.Username)
            @Html.DisplayFor(s => s.Username, new { @class = "form-control", @Value = Model.Username })
            @Html.ValidationMessageFor(s => s.Username)

        </div>
        <div class="form-group">
            @Html.LabelFor(s => s.Password)
            @Html.DisplayFor(s => s.Password, new { @class = "form-control", @Value = Model.Password })
            @Html.ValidationMessageFor(s => s.Password)
        </div>


        <div class="form-group">
            <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
        </div>
    </div>
}







